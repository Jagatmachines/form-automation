{
  "name": "rastriya-bank-form-automation-react",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "axios": "^0.18.0",
    "base64-img": "^1.0.4",
    "bootstrap": "^4.1.3",
    "final-form": "^4.9.1",
    "node-sass-chokidar": "^1.3.3",
    "noty": "^3.2.0-beta",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-bootstrap-table-next": "^1.3.0",
    "react-cropper": "^1.0.1",
    "react-dom": "^16.4.2",
    "react-dropzone-component": "^3.2.0",
    "react-final-form": "^3.6.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.3.1",
    "tesseract.js": "^1.0.10",
    "tesseract.ts": "^1.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/sass/*.scss -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/sass/style.min.scss -o src/styles/css/ --watch --recursive",
    "start-js": "REACT_APP_APIURL=http://localhost:8080 react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
